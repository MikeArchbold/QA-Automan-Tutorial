<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Login Tests">
	<test name="P1 Test">
		<groups>
			<run>
				<include name="p1"></include>
			</run>
		</groups>
		<classes>
			<class name="com.facebook.test.LoginTest"/>
		</classes>
	</test>
	<test name="P2 Test">
		<groups>
			<run>
				<include name="p2"></include>
			</run>
		</groups>
		<classes>
			<class name="com.facebook.test.LoginTest">
				<methods>
					<exclude name="fillOutPasswordField"/>
				</methods>
			</class>
		</classes>
	</test>
</suite>